#Fecha con año, mes, día, hora, minutos, segundos y microsegundos

import datetime

fecha = datetime.datetime.now()

print(fecha)

#Fecha personalizada (sin microsegundos)

import datetime

fecha = datetime.datetime(2023, 9, 9, 8, 48)

print(fecha)